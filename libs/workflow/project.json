{
  "root": "libs/workflow",
  "sourceRoot": "libs/workflow",
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["go test"],
        "cwd": "libs/workflow"
      }
    },
    "format:write": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["go fmt"],
        "cwd": "libs/workflow"
      }
    },
    "format:check": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["test -z $(gofmt -l .)"],
        "cwd": "libs/workflow"
      }
    }
  }
}
